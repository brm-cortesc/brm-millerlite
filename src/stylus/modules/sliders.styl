#myCarousel
	.carousel
		
		&-control
			cursor pointer
			
		&-indicators
			display none
	
#myCarousel-log
	background white	

	.carousel
		&-indicators
			.active
				background-color Mlred

				&:after
					display none

	.ct-slide
		height 9rem
		padding 1% 0 0 0
		width 100%
		
		
	.log-w-2 div
		display inline-block
		vertical-align top
		
	.pic-ct-2
		background-color Mldark
		border-radius 100%
		height 3rem
		margin 0% 0 0 0
		margin-top 1rem
		overflow hidden
		width 3rem
		

		img
			display block
			max-width 200%

	.txt-ct-2
		height auto
		padding 2% 0
		width 79%
			

		