@font-face
  font-family 'icomoon'
  src url('../fonts/icomoon.eot?6x01tp')
  src url('../fonts/icomoon.eot?6x01tp#iefix') format('embedded-opentype'),
      url('../fonts/icomoon.ttf?6x01tp') format('truetype'),
      url('../fonts/icomoon.woff?6x01tp') format('woff'),
      url('../fonts/icomoon.svg?6x01tp#icomoon') format('svg')
  font-weight normal
  font-style normal


[class^="icon-"], [class*=" icon-"]
  font-family 'icomoon' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

